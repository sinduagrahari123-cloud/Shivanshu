        "#Q.7\n",
        "A=[]\n",
        "B=[]\n",
        "C=[]\n",
        "\n",
        "r=int(input(\"Enter the no. of rows\"))\n",
        "c=int(input(\"Enter the no. of columns\"))\n",
        "if r==c:\n",
        "  print(\"Enter the elements of first matrix\")\n",
        "  for i in range(r):\n",
        "    A.append([])\n",
        "    for j in range(c):\n",
        "      x=int(input(\"Enter the element of row\"))\n",
        "      A[i].append(x)\n",
        "  print(\"Enter the elements of second matrix\")\n",
        "  for k in range(r):\n",
        "    B.append([])\n",
        "    for l in range(c):\n",
        "      y=int(input(\"Enter the element of row\"))\n",
        "      B[k].append(y)\n",
        "  for m in range(r):\n",
        "    C.append([])\n",
        "    for n in range(c):\n",
        "      p=A[m][n]+B[m][n]\n",
        "      C[m].append(p)\n",
        "  for q in C:\n",
        "    print(q)\n",
        "else:\n",
        "  print(\"Rows and columns should be equal\")\n",
        "\n",
        "\n",
