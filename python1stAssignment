        "#Q.8\n",
        "a=eval(input(\"Enter the list of actuall values\"))\n",
        "b=eval(input(\"Enter the list of predicted values\"))\n",
        "\n",
        "c=len(a)\n",
        "d=len(b)\n",
        "if c==d:\n",
        "  s=0\n",
        "  e=0\n",
        "  for i in range(c):\n",
        "    d=(a[i]-b[i])\n",
        "    if d<0:\n",
        "      d=d*(-1)\n",
        "    s=s+d\n",
        "  MAE=s/c\n",
        "  for j in range(c):\n",
        "    e=e+((a[j]-b[j])**2)\n",
        "  MSE=e/c\n",
        "  print(\"MAE is\",MAE)\n",
        "  print(\"MSE is\",MSE)\n",
        "else:\n",
        "  print(\"Both the lists should be equal\")\n",
        "\n",
        "\n",
        "\n",
