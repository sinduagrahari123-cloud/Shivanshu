        "#Q.9\n",
        "#only for 3x3 and 4x4\n",
        "\n",
        "a=eval(input(\"Enter the matrix\"))\n",
        "r=len(a)\n",
        "c=len(a[0])\n",
        "\n",
        "if r==c:\n",
        "  if r==3:\n",
        "    k=(a[0][0])*((a[1][1])-a[2][2])\n",
        "    l=(a[0][1])*((a[1][0])-a[2][0])\n",
        "    m=(a[0][2])*((a[1][0])-a[2][0])\n",
        "    z=(k-l+m)\n",
        "    print(\"The result is \",z)\n",
        "  if r==2:\n",
        "    k=a[0][0]*a[1][1]\n",
        "    l=a[0][1]*a[1][0]\n",
        "    z=(k-l)\n",
        "    print(\"The result is \",z)\n",
        "else:\n",
        "   print(\"only for 3x3 and 4x4\")\n",
        "\n",
